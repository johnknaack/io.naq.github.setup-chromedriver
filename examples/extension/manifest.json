{
  "manifest_version": 3,
  "name": "Simple Extension",
  "version": "1.0",
  "description": "Simple chrome extension to use as a test environment",
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
